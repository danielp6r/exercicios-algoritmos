algoritmo "69"
var
   i, termo, razao: inteiro
inicio
      escreva("Digite o primeiro termo da PA:")
      leia(termo)
      escreva("Digite a razão da PA:")
      leia(razao)
      
         para i <- 1 ate 10 faca
              escreva(termo)
              termo <- termo + razao
         fimpara

fimalgoritmo
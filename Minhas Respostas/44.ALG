algoritmo "44"
var
   v, vn, incremento: inteiro
inicio
      escreval("Digite o valor inicial:")
      leia(v)
      escreval("Contar até:")
      leia (vn)
      escreval("De quanto em quanto?")
      leia (incremento)
      
      enquanto (v <= vn) faca
               escreva (v)
               v <- v + incremento
      fimenquanto
      
      escreva(" Acabou!")

fimalgoritmo
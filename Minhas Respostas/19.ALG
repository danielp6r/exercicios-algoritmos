algoritmo "media"
var
   n1, n2: real

inicio
      escreval("Digite a primeira nota")
      leia(n1)
      escreval("Digite a segunda nota")
      leia(n2)
      se ((n1 + n2) / 2 >= 7) entao
         escreval ("Bom aproveitamento.")
      senao
           escreval("Mal aproveitamento.")
      fimse

fimalgoritmo